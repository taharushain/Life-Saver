<div id="wrapper">


	<%= render "partials/super_user_nav" %>
	<div id="page-wrapper" >

		<div class="container-fluid"  align="center">

			<!-- Page Heading -->
			<div class="row">
				<div class="col-lg-12">
					<h1 class="page-header">
						<small>Super User</small>
					</h1>
				</div>
			</div>
			<!-- /.row -->


			<div class="row">
				<div class="col-md-12">
					<%= render "partials/alerts" %>
				</div>
			</div>

			<!-- Contents -->
			<h2>Edit Bed</h2>
			<div class="form-wrapper" style="padding:30px;">
				<%= simple_form_for @bed, html: { class: 'form-horizontal', multipart: true } do |f| %>
				<div class="form-group text-center">
					<%= f.input :name %>
				</div>
				<div class="form-group text-center">
					<%= f.button :submit, class: 'btn-success' %>
				</div>
				<% end %>
			</div>

			
		</div>
		<!-- /.container-fluid -->

	</div>
	<!-- /#page-wrapper -->

</div>